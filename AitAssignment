16. Program to perform the CRUD Operations using PHP Script.
Program
<?php

// Database connection settings
$host = "localhost";
$username = "root";
$password = "";
$database = "phpcrud";

// Create a new record
function createRecord($name, $email) {
    global $host, $username, $password, $database;

    $conn = mysqli_connect($host, $username, $password, $database);

    $name = mysqli_real_escape_string($conn, $name);
    $email = mysqli_real_escape_string($conn, $email);

    $query = "INSERT INTO users (name, email) VALUES ('$name', '$email')";
    mysqli_query($conn, $query);

    mysqli_close($conn);
}

// Read all records
function readRecords() {
    global $host, $username, $password, $database;

    $conn = mysqli_connect($host, $username, $password, $database);

    $query = "SELECT * FROM users";
    $result = mysqli_query($conn, $query);

    $rows = [];
    while ($row = mysqli_fetch_assoc($result)) {
        $rows[] = $row;
    }

    mysqli_close($conn);

    return $rows;
}

// Update a record
function updateRecord($id, $name, $email) {
    global $host, $username, $password, $database;

    $conn = mysqli_connect($host, $username, $password, $database);

    $id = mysqli_real_escape_string($conn, $id);
    $name = mysqli_real_escape_string($conn, $name);
    $email = mysqli_real_escape_string($conn, $email);

    $query = "UPDATE users SET name='$name', email='$email' WHERE id='$id'";
    mysqli_query($conn, $query);

    mysqli_close($conn);
}

// Delete a record
function deleteRecord($id) {
    global $host, $username, $password, $database;

    $conn = mysqli_connect($host, $username, $password, $database);

    $id = mysqli_real_escape_string($conn, $id);

    $query = "DELETE FROM users WHERE id='$id'";
    mysqli_query($conn, $query);

    mysqli_close($conn);
}

// Usage examples
// Uncomment the following lines to test the CRUD operations

// Create a new record
// createRecord("Vaishnavi Choudhary", "Vaishnavichoudhary200@gmail.com");
// createRecord("Shravani Sonawane", "shravani@gmail.com");
// createRecord("Shweta Chothe", "shweta@gmail.com");
// createRecord("Vaishnavi Pagire", "Vaishnavi@gmail.com");


// Read all records
// $users = readRecords();
// foreach ($users as $user) {
//     echo "ID: " . $user['id'] . "<br>";
//     echo "Name: " . $user['name'] . "<br>";
//     echo "Email: " . $user['email'] . "<br>";
//     echo "<br>";
// }

// Update a record
// updateRecord(2, "Shravani Sonawane", "shravani30000@gmail.com");

// Delete a record
// deleteRecord(3);
?>
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
