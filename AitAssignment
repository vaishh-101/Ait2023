 if(isset($_POST['uname'])){ 
 session_start(); 
 if ($_SESSION['uname'] === $_POST['uname'] and $_SESSION['pass'] === $_POST['pass']){  echo " Welcome " . $_SESSION['uname'] ." , Login is successful"; 
 echo "<a href='link.html'>welcome</a>"; 
 } else{ 
 echo "Login Failed"; 
