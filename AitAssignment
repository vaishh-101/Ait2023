Verify.php 
<?php  
 $username = $_POST['uname']; 
 $password = $_POST['pass']; 
 if(isset($_POST['uname'])){ 
 session_start(); 
 $_SESSION["uname"]=$username; 
 $_SESSION["pass"]=$password; 
 echo "Session Started ";  
 echo "<a href='login.html'>Login now</a>"; 
 }else{ 
 echo "Please Enter Valid Details"; 
 } 
?> 
